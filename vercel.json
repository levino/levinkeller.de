{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "silent": true
  },
  "buildCommand": "git fetch --unshallow || true && npm run build",
  "redirects": [
    { "source": "/", "destination": "/de", "permanent": false },
    {
      "source": "/blog/de/:path*",
      "destination": "/de/blog/:path*",
      "permanent": true
    },
    {
      "source": "/docs/de/:path*",
      "destination": "/de/docs/:path*",
      "permanent": true
    },
    {
      "source": "/de/blog/2023-05-23-windkraft",
      "destination": "/de/docs/umwelt/windkraft"
    }
  ]
}
